{
    "version":2,
    "builds":[
        {
            "src":"der/src/main.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"der/src/main.js",
            "methods":[
                "GET",
                "POST",
                "PUT",
                "PATH",
                "DELETE"
            ]
        }
    ]
}