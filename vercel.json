{
    "version":2,
    "builds":[
        {
            "src": "dist/**/*",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest": "/dist/$1",
            "methods":[
                "GET",
                "POST",
                "PUT",
                "PATH",
                "DELETE"
            ]
        }
    ]
}